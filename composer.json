{
    "name": "kwaadpepper/sitemap-refresh",
    "description": "Create sitemap.xml",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Munsch",
            "email": "github@jeremydev.ovh",
            "homepage": "https://jeremydev.ovh"
        }
    ],
    "keywords": [
        "Laravel",
        "Sitemap"
    ],
    "require": {
        "illuminate/support": "~11",
        "laravel/framework": "~11",
        "spatie/laravel-sitemap": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "Kwaadpepper\\SitemapRefresh\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kwaadpepper\\SitemapRefresh\\SitemapRefreshServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "slevomat/coding-standard": "^8.15",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^9.0"
    }
}
